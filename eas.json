{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_MAPS_API_KEY": "@secret(GOOGLE_MAPS_API_KEY)"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_MAPS_API_KEY": "@secret(GOOGLE_MAPS_API_KEY)"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_MAPS_API_KEY": "@secret(GOOGLE_MAPS_API_KEY)"
      }
    }
  },
  "submit": {
    "production": {}
  }
}